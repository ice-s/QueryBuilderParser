{
    "name": "ice-s/query-builder-parser",
    "homepage": "https://github.com/ice-s/QueryBuilderParser",
    "version": "1.5.7",
    "description": "Build complex Eloquent & QueryBuilder queries automatically when using jQuery-QueryBuilder",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Son",
            "email": "nguyentruongson93@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
          "Ices\\QueryBuilderParser\\": "src/QueryBuilderParser/"
        }

    },
    "minimum-stability": "dev",
    "require": {
        "illuminate/database": "^6.0||4.1 - 6.0"
    },
    "suggest": {
        "jenssegers/mongodb": "Use QueryBuilderParser with MongoDB"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3|^7.0",
        "mockery/mockery": "^1.2.3|^0.9.4"
    }
}
